Conn * -> interface
	char *name
	Dev *dev
	void *inst
	Qid qid

Dev *
	Conn *(*dup)(Conn *, char *name)
	ssize_t (*pread)(Conn *, void *, size_t, off_t)
	ssize_t (*pwrite)(Conn *, const void *, size_t, off_t)
	ssize_t (*stat)(Conn *, void *, size_t)
	ssize_t (*wstat)(Conn *, const void *, size_t)
	int (*ioctlp)(Conn *, int, void *, size_t)
	int (*ioctli)(Conn *, int, int);
	int (*open)(Conn *, int, int)
	int (*walk)(Conn *, char *pel)
	void *(*mmap)(Conn *, off_t, size_t)

MntPt * -> implementation of Conn
	when reading:
		signal(mntpt->free)
		wait(mntpt->canproc)
		pop from mntpt->buf
	when writing:
		signal(buf->finished)

Mnt * -> implementation of Conn
	when asking:
		wait(mntpt->free)
		push to mntpt->buf
		signal(mntpt->canproc)
		wait(buf->finished)

Pipe * -> implementation of Conn

